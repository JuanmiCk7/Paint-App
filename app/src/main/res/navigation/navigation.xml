<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    app:startDestination="@id/titleFragment">

    <fragment
        android:id="@+id/drawingFragment"
        android:name="com.example.paintfinal.drawing.DrawingFragment"
        android:label="DrawingFragment" />
    <fragment
        android:id="@+id/titleFragment"
        android:name="com.example.paintfinal.TitleFragment"
        android:label="TitleFragment" >
        <action
            android:id="@+id/action_titleFragment_to_drawingFragment"
            app:destination="@id/drawingFragment" />
    </fragment>
</navigation>